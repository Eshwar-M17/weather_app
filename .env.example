# Weather App Environment Variables Example
# Copy this file to .env and replace with your actual values

# OpenWeatherMap API Key
# Get your API key from: https://home.openweathermap.org/api_keys
OPEN_WEATHER_MAP_API_KEY=your_api_key_here

# Default City (optional, defaults to 'Bangalore' if not specified)
DEFAULT_CITY=Bangalore

# Cache Duration in Hours (optional, defaults to 1)
CACHE_DURATION_HOURS=1
